#ifndef RTC_H_
#define RTC_H_

#include "main.h"

extern RTC_HandleTypeDef hrtc;

namespace blib
{
    class Rtc
    {
        public:
            Rtc();
            ~Rtc();

        private:
            uint8_t mHour;
            uint8_t mMinute;
            uint8_t mDate;
            uint8_t mMonth;
            uint8_t mYear;
    };
}

#endif /* RTC_H_ */
